    <!-- http://emmo.info/emmo/middle/materials#EMMO_a2b006f2_bbfd_4dba_bcaa_3fca20cd6be1 -->

    <owl:Class rdf:about="http://emmo.info/emmo/middle/materials#EMMO_a2b006f2_bbfd_4dba_bcaa_3fca20cd6be1">
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo/middle/materials#EMMO_8b0923ab_b500_477b_9ce9_8b3a3e4dc4f2"/>
        <annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>A continuum characterized by structural rigidity and resistance to changes of shape or volume, that retains its shape and density when not confined.</annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <rdfs:label xml:lang="en">Solid</rdfs:label>
    </owl:Class>
    


    <!-- http://emmo.info/emmo/middle/materials#EMMO_db03061b_db31_4132_a47a_6a634846578b -->

    <owl:Class rdf:about="http://emmo.info/emmo/middle/materials#EMMO_db03061b_db31_4132_a47a_6a634846578b">
        <annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>A standalone atom with an unbalanced number of electrons with respect to its atomic number.</annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
