    <!-- http://emmo.info/domains/emmo-material.owl#EMMO_3397f270_dfc1_4500_8f6f_4d0d85ac5f71 -->

    <owl:Class rdf:about="http://emmo.info/domains/emmo-material.owl#EMMO_3397f270_dfc1_4500_8f6f_4d0d85ac5f71">
