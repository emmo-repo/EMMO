        <rdfs:label xml:lang="en">DescriptiveProperty</rdfs:label>
    </owl:Class>
    


    <!-- http://emmo.info/domains/emmo-physical-properties.owl#EMMO_dd4a7f3e_ef56_466c_ac1a_d2716b5f87ec -->

    <owl:Class rdf:about="http://emmo.info/domains/emmo-physical-properties.owl#EMMO_dd4a7f3e_ef56_466c_ac1a_d2716b5f87ec">
