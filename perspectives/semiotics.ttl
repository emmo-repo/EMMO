                                                             owl:onProperty :EMMO_b8d05d29_9eea_47db_a20b_0b83ceb6de93 ;
                                                             owl:someValuesFrom :EMMO_054af807_85cd_4a13_8eba_119dfdaaf38b
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
